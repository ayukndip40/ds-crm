              const contactCity = button.dataset.city;
              const contactCountry = button.dataset.country;

              // Populate the form fields
              document.getElementById("update-name").value = contactName;
              document.getElementById("update-email").value = contactEmail;
              document.getElementById("update-phone").value = contactPhone;
              document.getElementById("update-address").value = contactAddress;
              document.getElementById("update-city").value = contactCity;
              document.getElementById("update-country").value = contactCountry;

              // Show the update contact modal
              $("#updateContactModal").modal("show"); // Using jQuery to show the modal
            }
          });
        }
      });

      // Handle the form submission for updating contact
      document
        .getElementById("update-contact-form")
        .addEventListener("submit", async (e) => {
          e.preventDefault();

          // Gather the updated contact data from the form
          const updatedContact = {
            name: document.getElementById("update-name").value,
            email: document.getElementById("update-email").value,
            phone: document.getElementById("update-phone").value,
            address: document.getElementById("update-address").value,
            city: document.getElementById("update-city").value,
            country: document.getElementById("update-country").value,
          };

          // Here you would send the updatedContact data to your server
          // using a fetch call with the appropriate endpoint and method.
          const contactId = document.querySelector(
            ".update-contact-btn[data-id]"
          ).dataset.id; // Fetch the contact ID from the button
          try {
            // Fetch tokens from local storage
            const accessToken = localStorage.getItem("accessToken");
            const refreshToken = localStorage.getItem("refreshToken");
            console.log("Fetched tokens from local storage:", {
              accessToken,
              refreshToken,
            });
            const response = await fetch(
              `http://localhost:3000/auth/contacts/contacts/${contactId}`,
              {
                method: "PUT", // Change to the appropriate method for updating
                headers: {
                  "Content-Type": "application/json",
                  Authorization: `Bearer ${accessToken}`,
                  "x-refresh-token": refreshToken, // Include the refresh token if available
                },
                body: JSON.stringify(updatedContact),
              }
            );

            const result = await response.json();
            console.log("Response from server:", result);
            if (response.ok) {
              // Display success message and close modal
              document.getElementById("response-message").innerHTML =
                '<div class="alert alert-success">Contact updated successfully!</div>';
              $("#updateContactModal").modal("hide");
            } else {
              document.getElementById(
                "response-message"
              ).innerHTML = `<div class="alert alert-danger">Error: ${result.message}</div>`;
            }
          } catch (error) {
            console.error("Error updating contact:", error);
            document.getElementById("response-message").innerHTML =
              '<div class="alert alert-danger">Failed to update contact</div>';
          }
        });
    </script>
  </body>
</html>
