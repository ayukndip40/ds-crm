          );

          console.log("Contacts fetch response status:", response.status);
          const data = await response.json();
          console.log("Contacts fetch response data:", data);

          if (response.ok) {
            console.log("Data fetch was successful.");
            const contacts = data;
            console.log("Contacts data extracted:", contacts);

